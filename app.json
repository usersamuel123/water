{
  "expo": {
    "name": "WaterTracker",
    "slug": "watertracker",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "bundleIdentifier": "com.example.watertracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.example.watertracker",
      "useNextNotificationsApi": true
    },
    "plugins": [
      "expo-font",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "18273edd-7c01-4e1f-be2a-70817917220d"
      }
    },
    "owner": "samuele2905"
  }
}
